{
  "name": "let-find",
  "displayName": "Let Find",
  "description": "Extension with the search support for Visual Studio Code",
  "version": "0.0.6",
  "publisher": "MaratZimnurov",
  "repository": "https://github.com/Zimtir/let-find",
  "engines": {
    "vscode": "^1.40.0"
  },
  "icon": "assets/logo-128x128.png",
  "extensionDependency": [
    "auchenberg.vscode-browser-preview"
  ],
  "activationEvents": [
    "onCommand:extension.find",
    "onCommand:extension.findBySelection"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.find",
        "title": "Let find",
        "icon": "vs-search",
        "category": "Search"
      },
      {
        "command": "extension.findBySelection",
        "title": "Let find by selection",
        "icon": "vs-search",
        "category": "Search"
      }
    ],
    "keybindings": [
      {
        "command": "extension.find",
        "key": "ctrl+l",
        "mac": "cmd+l"
      },
      {
        "command": "extension.findBySelection",
        "key": "ctrl+shi∆ít+l",
        "mac": "cmd+shift+l"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.findBySelection",
          "alt": "extension.find",
          "group": "z_commands",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "backend": "nodemon --watch 'src/server/**/*.ts' --exec 'ts-node' src/server/app.ts",
    "pbl": "vsce package"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@types/request-promise-native": "^1.0.17",
    "axios": "^0.19.0",
    "cors": "^2.8.4",
    "morgan": "^1.8.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "timeago.js": "^4.0.1"
  }
}
