{
  "name": "let-find",
  "displayName": "Let Find",
  "description": "Extension with the search support for Visual Studio Code",
  "version": "0.0.7",
  "publisher": "MaratZimnurov",
  "repository": "https://github.com/Zimtir/let-find",
  "engines": {
    "vscode": "^1.40.0"
  },
  "icon": "assets/logo-128x128.png",
  "extensionDependency": [
    "auchenberg.vscode-browser-preview"
  ],
  "activationEvents": [
    "onCommand:extension.find",
    "onCommand:extension.findBySelection"
  ],
  "main": "./out/index.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.find",
        "title": "Let find",
        "icon": "vs-search",
        "category": "Search"
      },
      {
        "command": "extension.findBySelection",
        "title": "Let find by selection",
        "icon": "vs-search",
        "category": "Search"
      }
    ],
    "keybindings": [
      {
        "command": "extension.find",
        "key": "ctrl+l",
        "mac": "cmd+l"
      },
      {
        "command": "extension.findBySelection",
        "key": "ctrl+shi∆ít+l",
        "mac": "cmd+shift+l"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.findBySelection",
          "alt": "extension.find",
          "group": "z_commands",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "pbl": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.40.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3",
    "@types/request-promise-native": "^1.0.17"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "timeago.js": "^4.0.1"
  }
}
