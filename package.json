{
  "name": "let-find",
  "displayName": "Let Find",
  "description": "Extension with the search support for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "MaratZimnurov",
  "repository": "https://github.com/Zimtir/let-find",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.40.0"
  },
  "icon": "assets/logo-128x128.png",
  "extensionDependency": [
    "auchenberg.vscode-browser-preview"
  ],
  "activationEvents": [
    "onCommand:extension.find",
    "onCommand:extension.findBySelection"
  ],
  "galleryBanner": {
    "color": "#8647ae",
    "theme": "dark"
  },
  "keywords": [
    "msdn",
    "google",
    "stackoverflow",
    "search",
    "search inside vs code",
    "google inside vs code",
    "msdn inside vs code",
    "stackoverflow inside vs code"
  ],
  "main": "./out/index.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.find",
        "title": "Let find",
        "icon": "vs-search",
        "category": "Search"
      },
      {
        "command": "extension.findBySelection",
        "title": "Let find by selection",
        "icon": "vs-search",
        "category": "Search"
      }
    ],
    "keybindings": [
      {
        "command": "extension.find",
        "key": "ctrl+l",
        "mac": "cmd+l"
      },
      {
        "command": "extension.findBySelection",
        "key": "ctrl+shi∆ít+l",
        "mac": "cmd+shift+l"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.findBySelection",
          "alt": "extension.find",
          "group": "z_commands",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package",
    "publish": "vsce publish -p %PUBLISHER_TOKEN%",
    "pbl": "vsce package & vsce publish"
  },
  "devDependencies": {
    "@types/javascript-time-ago": "^2.0.0",
    "@types/request-promise-native": "^1.0.17",
    "@types/vscode": "^1.40.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.20.0",
    "typescript": "^3.7.3",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "javascript-time-ago": "^2.0.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  }
}
